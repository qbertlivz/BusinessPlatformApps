                "ConnectorName": "$save('dynamicsax')",
                "AxReportName": "$save('CreditAndCollectionsManagementReport')",
                "AxMeasurementName": "$save('CustCollectionsBIMeasurements')"
            },
            {
                "datastoreEntriesToValidate": [
                    "SqlConnectionString"
